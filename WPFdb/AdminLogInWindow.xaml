<Window x:Class="WPFdb.AdminLogInWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFdb"
        mc:Ignorable="d"
        Title="AdminLogInWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtAdminUserName" HorizontalAlignment="Left" Height="23" Margin="199,149,0,0" TextWrapping="Wrap" Text="admin" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txtAdminPassword" HorizontalAlignment="Left" Height="23" Margin="367,149,0,0" TextWrapping="Wrap" Text="admin123" VerticalAlignment="Top" Width="120"/>
        <Button Content="Log In as Admin" HorizontalAlignment="Left" Margin="382,210,0,0" VerticalAlignment="Top" Width="105" Click="Button_Click"/>
        <Label Content="Username" HorizontalAlignment="Left" Margin="199,123,0,0" VerticalAlignment="Top"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="367,123,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
